{
  "name": "rotten",
  "productName": "rotten",
  "version": "0.0.1",
  "description": "a demo for Electron + rot.js",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "build": "rollup -c rollup.config.js",
    "watch": "rollup -c rollup.config.dev.js --watch",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fralonra/rotten.git"
  },
  "keywords": [
    "electron",
    "rot.js"
  ],
  "author": "zoron (https://github.com/fralonra/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fralonra/rotten/issues"
  },
  "homepage": "https://github.com/fralonra/rotten#readme",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "rotten"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.45",
    "@electron-forge/maker-deb": "6.0.0-beta.45",
    "@electron-forge/maker-rpm": "6.0.0-beta.45",
    "@electron-forge/maker-squirrel": "6.0.0-beta.45",
    "@electron-forge/maker-zip": "6.0.0-beta.45",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-typescript": "^3.0.0",
    "electron": "9.1.0",
    "rollup": "^1.31.0",
    "rollup-plugin-terser": "^5.2.0",
    "rot-js": "^2.1.1",
    "typescript": "^3.7.5"
  },
  "standard": {
    "ignore": [
      "/src/dist"
    ]
  }
}
